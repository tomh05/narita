<p id="notice"><%= notice %></p>

<h1>App Uniques</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Event time</th>
      <th>App name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_uniques.each do |app_unique| %>
      <tr>
        <td><%= app_unique.event_time %></td>
        <td><%= app_unique.app_name %></td>
        <td><%= app_unique.username %></td>
        <td><%= link_to 'Destroy', app_unique, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-mini btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
